{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "Vo.Ci. San Marino, Associazione VOlontari CIvici San Marino",
  "version": "1.0.1",
  "author": "Simone Foschi <s.foschi@gmail.com>",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "decap-cms-app": "^3.5.0",
    "dotenv": "^16.4.7",
    "fs": "^0.0.1-security",
    "fslightbox-react": "^1.7.6",
    "gatsby": "^5.14.1",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-offline": "^6.13.3",
    "gatsby-plugin-sass": "^6.13.1",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-source-google-calendar": "^1.1.3",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.14.0",
    "graphql": "^16.10.0",
    "html-react-parser": "^5.1.12",
    "netlify-identity-widget": "^1.9.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-quill": "^2.0.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "gatsby-plugin-postcss": "^6.13.1",
    "postcss": "^8.4.40",
    "prettier": "^2.8.8"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "Private Repo, all rights reserved &copy; Simone Foschi",
  "scripts": {
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "img-move":"cp -r content/assets/images/blog/* static/img/",
    "build-prod": "npm run img-move && gatsby build && gatsby serve",
    "build-dev": "npm run img-move && gatsby build && gatsby develop",
    "deploy": "netlify deploy --prod --dir=public",
    "clean": "gatsby clean",
    "clean-public": "rm -rf ./public",
    "clean-cache": "rm -rf ./cache",
    "clean-node": "rm -rf ./node_modules && rm package-lock.json",
    "clean-all": "npm run clean && npm run clean-cache && npm run clean-public && npm run clean-node",
    "clean-build-run": "npm run clean-all && npm install && npm run build-dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
